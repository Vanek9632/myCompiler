int fibonacci(int n)
{
    if (n == 0) { return 0; }
    if (n == 1 || n == 2) { return 1; }

    return fibonacci(n-1)+fibonacci(n-2);
}

int main()
{
	printf("%i\n", fibonacci(0));
	printf("%i\n", fibonacci(10));
	printf("%i\n", fibonacci(29));
	printf("%i\n", fibonacci(36));
	printf("%i\n", fibonacci(40));
}